{
  "name": "cosmo_info",
  "version": "2.1.0",
  "description": "CosmoDance чат-ассистент с парсингом расписания и цен",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "test": "node -e \"import('./cosmo-parser.js').then(m => new m.default().getSchedule().then(d => console.log('✅ Парсер работает:', Object.keys(d))))\"",
    "clear-cache": "node -e \"import('./cosmo-parser.js').then(m => new m.default().clearCache())\""
  },
  "keywords": [
    "chatbot",
    "dance-studio",
    "cosmodance",
    "schedule-parser",
    "price-parser"
  ],
  "author": "CosmoDance Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CosmoDance/Cosmo_info.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.3"
  }
}

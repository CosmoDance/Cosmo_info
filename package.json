{
  "name": "cosmo_info",
  "version": "1.0.0",
  "description": "CosmoDance чат-ассистент с самообучением",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:watch": "nodemon --watch server.js --watch *.js server.js",
    "prod": "NODE_ENV=production node server.js",
    "test": "node --test test/*.test.js",
    "lint": "eslint server.js",
    "deploy": "npm run lint && npm test"
  },
  "keywords": [
    "chatbot",
    "dance-studio",
    "ai-assistant",
    "openai",
    "cosmodance"
  ],
  "author": "CosmoDance Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CosmoDance/Cosmo_info.git"
  },
  "bugs": {
    "url": "https://github.com/CosmoDance/Cosmo_info/issues"
  },
  "homepage": "https://cosmodance.ru",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.18.2",
    "openai": "^4.30.0",
    "winston": "^3.12.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
